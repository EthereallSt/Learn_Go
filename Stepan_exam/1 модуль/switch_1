package main

import "fmt"

func switch1() {
	var a int
	fmt.Scan(&a)

	switch {
	case a < 10:
		fmt.Println(a)
	case a <= 99 && a >= 10:
		a = a / 10
		fmt.Println(a)
	case a <= 999:
		a = a / 100
		fmt.Println(a)
	case a <= 9999:
		a = a / 1000
		fmt.Println(a)
	case a == 10000:
		fmt.Println(1)
	}
}
